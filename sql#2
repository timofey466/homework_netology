create table if not exists songer(
  Id serial primary key,
  Name varchar(40) not null ,
  Generel text 
);

create table if not exists albom(
  first_id integer references songer(Id),
  Id serial primary key,
  Name varchar(40) not null ,
  Generel varchar(30) not null,
  year integer not null
);

create table if not exists song(
  second_id integer references albom(Id),
  Id serial primary key,
  Name varchar(40) not null ,
  Generel varchar(30) not null
  time integer not null
);

create table if not exists general(
  Id serial primary key,
  third_id integer references songer(Id),
  fourth_id integer references albom(Id),
  fiveth_id integer references song(Id),
  bio varchar(40) unique not null
);
